{
  "name": "tandem-docs",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "type": "module",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "start": "turbo start",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "db:push": "turbo -F @tandem-docs/db db:push",
    "db:studio": "turbo -F @tandem-docs/db db:studio",
    "db:generate": "turbo -F @tandem-docs/db db:generate",
    "db:migrate": "turbo -F @tandem-docs/db db:migrate",
    "db:seed": "turbo -F @tandem-docs/db db:seed"
  },
  "dependencies": {
    "@tandem-docs/env": "workspace:*",
    "dotenv": "catalog:",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@tandem-docs/config": "workspace:*",
    "@types/node": "^22.13.14",
    "turbo": "^2.6.3",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.2.0"
}
