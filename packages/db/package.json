{
  "name": "@tandem-docs/db",
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    },
    "./*": {
      "default": "./src/*.ts"
    },
    "./schema": {
      "default": "./src/schema/index.ts"
    },
    "./schema/auth": {
      "default": "./src/schema/auth.ts"
    },
    "./schema/chat": {
      "default": "./src/schema/chat.ts"
    },
    "./schema/relations": {
      "default": "./src/schema/relations.ts"
    },
    "./schema/project": {
      "default": "./src/schema/project.ts"
    }
  },
  "scripts": {
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:studio": "drizzle-kit studio",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "npx tsx src/seed.ts"
  },
  "dependencies": {
    "@tandem-docs/env": "workspace:*",
    "dotenv": "catalog:",
    "drizzle-orm": "^0.45.1",
    "pg": "^8.17.1",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@tandem-docs/config": "workspace:*",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.8",
    "typescript": "catalog:"
  }
